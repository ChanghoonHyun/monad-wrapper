{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-es2015-template-literals",
    "transform-async-to-generator",
    "transform-strict-mode",
    "babel-plugin-transform-object-rest-spread"
  ]
}